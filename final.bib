@article{Worthington2012,
abstract = {We present a two-dimensional velocity model to constrain crustal thickness and composition of the Yakutat terrane in the northern Gulf of Alaska. The model was constructed using seismic reflection and refraction data along a {\&}{\#}8764;455 km onshore-offshore profile. Our model shows that the crystalline crust composing the Yakutat terrane is wedge-shaped, with crustal thickness increasing west to east from {\&}{\#}8764;15 km to {\&}{\#}8764;30 km. Crustal velocity and structure are continuous across the terrane, with lower crustal velocities {\textgreater}7 km/s, suggesting that the Yakutat terrane is an oceanic plateau across its entire offshore extent rather than a composite oceanic-continental terrane as previously proposed. The thickest Yakutat crust is entering the adjacent St. Elias orogen where elevated exhumation rates and concentrated seismicity in this vicinity are likely influenced by incipient Yakutat-North America collision. Our model includes a {\&}{\#}8764;8 km thick low-velocity crustal cap extending across the eastern portion of the profile where shallow basement is imaged on marine seismic reflection data. We interpret this cap as a lithified, metamorphosed remnant accretionary prism, providing evidence of a previous attempt at Yakutat subduction along its eastern margin prior to current emplacement at the southern Alaska margin.},
author = {Worthington, Lindsay L. and {Van Avendonk}, Harm J. A. and Gulick, Sean P. S. and Christeson, Gail L. and Pavlis, Terry L.},
doi = {10.1029/2011JB008493},
file = {:run/media/iannesbitt/3a033774-4d6e-4e69-b8f4-c34bb7bd2e35/home/iannesbitt/MASTERS/literature/Worthington et al/2012/Worthington et al. - 2012 - Crustal structure of the Yakutat terrane and the evolution of subduction and collision in southern Alaska.pdf:pdf},
isbn = {0148-0227},
issn = {01480227},
journal = {Journal of Geophysical Research: Solid Earth},
month = {jan},
number = {B1},
pages = {n/a--n/a},
title = {{Crustal structure of the Yakutat terrane and the evolution of subduction and collision in southern Alaska}},
url = {http://doi.wiley.com/10.1029/2011JB008493},
volume = {117},
year = {2012}
}
@article{Spotila2004,
abstract = {An emerging paradigm that equates glaciers to ''buzz saws'' of exceptional erosional efficiency has been strengthened by shortterm ({\textless}10 2 yr) sediment yields from southern Alaska. New lowtemperature cooling ages from this area, the glaciated Chugach-St. Elias Mountains, constrain long-term (10 6 yr) exhumation rates. Vertically averaged exhumation rates reach ;3 mm/yr, but are an order of magnitude lower than rates based on short-term sediment yields. Whereas these exhumation rates are not exceptional for orogenic belts, denudation patterns are strongly correlated with the distribution of glaciers, and erosion appears to keep pace with convergence and uplift. These findings imply a coupling between glacially dominated erosion and tectonics.},
author = {Spotila, James A. and Buscher, Jamie T. and Meigs, Andrew J. and Reiners, Peter W.},
