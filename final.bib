doi = {10.1130/G2252A.1},
file = {:run/media/iannesbitt/3a033774-4d6e-4e69-b8f4-c34bb7bd2e35/home/iannesbitt/MASTERS/literature/Enkelmann, Garver, Pavlis/2008/Enkelmann, Garver, Pavlis - 2008 - Rapid exhumation of ice-covered rocks of the Chugach-St. Elias orogen, Southeast Alaska.pdf:pdf},
