@article{Hooyer2012,
abstract = {A principal assumption of models that describe bedrock quarrying by glaciers is that cracks in the bed are small and isolated. These cracks are assumed to grow slowly in response to glacial loading to eventually delimit the areas of quarried surfaces. If this assumption is correct, then quarried-surface orientations will be controlled by orientations of principal stresses in the bed associated with sliding and resultant ice-bed separation downglacier from rock ledges and roches moutonn{\'{e}}es, rather than by orientations of joints that predate glacial loading. To test this hypothesis, we compared orientations of quarried surfaces and preglacial joints in nine recently deglaciated forefields of glaciers in Canada and Switzerland with different bedrock lithologies and bedforms. In all but one forefield, quarried surfaces and major joint sets are coincident, with orientations of quarried surfaces bearing no systematic relationship to sliding direction other than their obvious tendency to not face upglacier. A difference in sliding direction of 64° between two areas of one glacier forefield had no effect on the orientation of quarried surfaces, with the same joint set exploited in both areas. These observations motivate a new characterization of subglacial bedrock that differs from that of existing quarrying models. The bed is idealized as a group of rock blocks separated by preglacial joints. Slow crack growth necessary for quarrying would be confined to small bodies of rock (bridges) that connect the faces of joints. This model of the bed does not allow it to be treated as an intact elastic solid with hydraulically isolated cracks as is usually assumed, which would have a profound influence on characterization of quarrying mechanics. {\textcopyright} 2012 Elsevier B.V.},
author = {Hooyer, Thomas S. and Cohen, Denis and Iverson, Neal R.},
doi = {10.1016/j.geomorph.2012.02.012},
file = {:run/media/iannesbitt/3a033774-4d6e-4e69-b8f4-c34bb7bd2e35/home/iannesbitt/MASTERS/literature/Hooyer, Cohen, Iverson/2012/Hooyer, Cohen, Iverson - 2012 - Control of glacial quarrying by bedrock joints.pdf:pdf},
isbn = {4142295594},
issn = {0169555X},
journal = {Geomorphology},
keywords = {Bedrock joints,Glacial erosion,Glacial quarrying,Quarrying models},
pages = {91--101},
publisher = {Elsevier B.V.},
title = {{Control of glacial quarrying by bedrock joints}},
url = {http://dx.doi.org/10.1016/j.geomorph.2012.02.012},
volume = {153-154},
year = {2012}
}
@article{Riihimaki2005,
abstract = {Glacial erosion is known to be driven by sliding, which occurs in$\backslash$nbrief episodes in the melt season. What is not known is how well$\backslash$nthis sliding history is reflected in the sediment output from a glacier$\backslash$nand to what degree temporary sediment storage in the glacier smoothes$\backslash$nthe history of sediment production. We document sediment evacuation$\backslash$nrates for three seasons at Bench Glacier, a small temperate alpine$\backslash$nglacier in the Chugach Range of Alaska. Two lines of evidence suggest$\backslash$nthat subglacial sediment storage is small. Strong hysteresis in the$\backslash$nrelationship between sediment and water discharge argues for sediment$\backslash$nexhaustion from the subglacial system on seasonal and flood event$\backslash$ntimescales. Uplift of the glacier surface during periods of enhanced$\backslash$nbasal motion reflects intimate contact of the glacier sole with up-glacier-inclined$\backslash$nstoss slopes of bedrock bumps. Sediment evacuation rates at Bench$\backslash$nGlacier should therefore closely track glacial erosion rates over$\backslash$nannual timescales. Much of the sediment emerges in a major pulse$\backslash$nthat closely follows the termination of enhanced sliding each year,$\backslash$nsuggesting that the conduits and well-connected cavities that allowed$\backslash$nremoval of englacial water to terminate sliding also promoted efficient$\backslash$ntransport of sediment. This small glacier efficiently modifies the$\backslash$nlandscape, lowering its bed by 1–2 mm/yr while sliding only 1–2 m/yr.$\backslash$nOur sparse measurements of instantaneous bed load discharge average$\backslash$n41{\%} of concurrent suspended sediment discharge. Assuming that half$\backslash$nof the suspended load derives from abrasion of subglacially quarried$\backslash$nclasts, roughly two thirds of the sediment evacuated can be attributed$\backslash$nto quarrying.},
author = {Riihimaki, Catherine A. and MacGregor, Kelly R. and Anderson, Robert Stewart and Anderson, Suzanne Prestrud and Loso, Michael G.},
doi = {10.1029/2004JF000189},
file = {:run/media/iannesbitt/3a033774-4d6e-4e69-b8f4-c34bb7bd2e35/home/iannesbitt/MASTERS/literature/Riihimaki et al/2005/Riihimaki et al. - 2005 - Sediment evacuation and glacial erosion rates at a small alpine glacier.pdf:pdf},
isbn = {0148-0227},
issn = {0148-0227},
journal = {Journal of Geophysical Research},
number = {F3},
pages = {F03003},
title = {{Sediment evacuation and glacial erosion rates at a small alpine glacier}},
url = {http://doi.wiley.com/10.1029/2004JF000189},
volume = {110},
year = {2005}
}
@article{Muskett2003,
abstract = {Digital elevation models (DEMs) of Bagley Ice Valley and Malaspina Glacier produced by (i) Intermap Technologies, Inc. (ITI) from airborne interferometric synthetic aperture radar (InSAR) data acquired 4–13 September 2000, (ii) the German Aerospace Center (DRL) from spaceborne InSAR data acquired by the Shuttle Radar Topography Mission (SRTM) 11–22 February 2000, and (iii) the US Geological Survey (USGS) from aerial photographs acquired in 1972/73, were differenced to estimate glacier surface elevation changes from 1972 to 2000. Spatially non-uniform thickening, 10 ± 7 m on average, is observed on Bagley Ice Valley (accumulation area) while non-uniform thinning, 47 ± 5 m on average, is observed on the glaciers of the Malaspina complex (mostly ablation area). Even larger thinning is observed on the retreating tidewater Tyndall Glacier. These changes have resulted from increased temperature and precipitation associated with climate warming, and rapid tidewater retreat.},
